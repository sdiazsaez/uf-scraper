{
    "name": "larangular/uf-scraper",
    "description": "scrapper support for CLF to CLP currency",
    "license": "MIT",
    "authors": [
        {
            "name": "Simon Diaz",
            "email": "sdiaz.sz@gmail.com"
        }
    ],
    "require": {
        "php": "^7.1.3",
        "larangular/support": "^0.1.3",
        "illuminate/support": "^5.7",
        "symfony/dom-crawler": "^4.2",
        "guzzlehttp/guzzle": "^6.3"
    },
    "require-dev": {
        "phpunit/phpunit": "^7.5",
        "orchestra/testbench": "~3.0"
    },
    "autoload": {
        "psr-4": {
            "Larangular\\UFScraper\\": "./src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Larangular\\UFScraper\\Tests\\": "./tests"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Larangular\\UFScraper\\UFScraperServiceProvider"
            ]
        }
    }
}
