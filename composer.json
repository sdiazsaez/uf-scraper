{
    "name": "larangular/uf-scraper",
    "description": "scrapper support for CLF to CLP currency",
    "license": "MIT",
    "authors": [
        {
            "name": "Simon Diaz",
            "email": "sdiaz.sz@gmail.com"
        }
    ],
    "require": {
        "php"                : "^7.3|^8.0",
        "illuminate/support" : "~5.8.0|^6.0|^7.0|^8.0",
        "larangular/support" : "^0.1.0|^0.2.0",
        "symfony/dom-crawler": "^4.2|^5.1",
        "guzzlehttp/guzzle"  : "^7.0.1",
        "ext-json"           : "*"
    },
    "require-dev": {
        "phpunit/phpunit": "^7.5|^9.3",
        "orchestra/testbench": "~3.0|^4.0|^5.0|^6.0"
    },
    "autoload": {
        "psr-4": {
            "Larangular\\UFScraper\\": "./src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Larangular\\UFScraper\\Tests\\": "./tests"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Larangular\\UFScraper\\UFScraperServiceProvider"
            ]
        }
    }
}
